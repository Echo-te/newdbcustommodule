<odoo>
    <record id="seq_customer" model="ir.sequence">
        <field name="name">Customer Sequence</field>
        <field name="code">oig.customer</field>
        <field name="prefix">CUST-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_agent" model="ir.sequence">
        <field name="name">Agent Sequence</field>
        <field name="code">oig.agent</field>
        <field name="prefix">AGT-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>
</odoo>
